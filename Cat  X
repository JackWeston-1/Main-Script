-- Scripts:

local function TQYWRT_fake_script() -- ToggleUI.LocalScript 
	local script = Instance.new('LocalScript', ToggleUI)

	local button = script.Parent
	local gui = button.Parent
	local mainFrame = gui:WaitForChild("Main")
	
	button.MouseButton1Click:Connect(function()
		mainFrame.Visible = true
		button.Visible = false
	end)
	
end
coroutine.wrap(TQYWRT_fake_script)()
local function HLXP_fake_script() -- ToggleUI.LocalScript 
	local script = Instance.new('LocalScript', ToggleUI)

	script.Parent.Active = true
	script.Parent.Draggable = true
end
coroutine.wrap(HLXP_fake_script)()
local function MHMKPW_fake_script() -- Server.LocalScript 
	local script = Instance.new('LocalScript', Server)

	local button = script.Parent
	local setting = button.Parent
	local scriptsFrame = setting:WaitForChild("ScriptsFrame")
	local serverFrame = setting:WaitForChild("ServerFrame")
	
	button.MouseButton1Click:Connect(function()
		serverFrame.Visible = true
		scriptsFrame.Visible = false
	end)
	
end
coroutine.wrap(MHMKPW_fake_script)()
