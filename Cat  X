local function THMQQTJ_fake_script() -- SettingButton.LocalScript 
	local script = Instance.new('LocalScript', SettingButton)

	local button = script.Parent
	local gui = button.Parent.Parent
	local settingTab = gui:WaitForChild("Setting")
	local editorTab = gui:WaitForChild("Editor") -- n·∫øu b·∫°n c√≥ nhi·ªÅu tab
	
	button.MouseButton1Click:Connect(function()
		local isVisible = not settingTab.Visible
		settingTab.Visible = isVisible
		editorTab.Visible = false -- t·∫Øt tab kh√°c
	end)
	
end
coroutine.wrap(THMQQTJ_fake_script)()
