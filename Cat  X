local function DGVUO_fake_script() -- CloseUI.LocalScript 
	local script = Instance.new('LocalScript', CloseUI)

	local button = script.Parent
	local main2 = button.Parent
	local main = main2.Parent
	local gui = main.Parent
	
	local toggleButton = gui:WaitForChild("ToggleUI")
	
	button.MouseButton1Click:Connect(function()
		main.Visible = false
		toggleButton.Visible = true
	end)
	
end
coroutine.wrap(DGVUO_fake_script)()
local function RGVFIY_fake_script() -- Editor.LocalScript 
	local script = Instance.new('LocalScript', Editor)

	local editorBox = script.Parent.TextScroll.EditorBox
	local lineNumbers = script.Parent.LineScroll.LineNumbers -- gi·ªù l√† TextLabel!
	local textScroll = script.Parent.TextScroll
	local lineScroll = script.Parent.LineScroll
	
	-- C·ªë ƒë·ªãnh chi·ªÅu cao 1 d√≤ng
	local lineHeight = editorBox.TextSize + 4 -- b·∫°n c√≥ th·ªÉ ch·ªânh s·ªë n√†y n·∫øu c·∫ßn
	
	local function updateLines()
		local lines = select(2, editorBox.Text:gsub("\n", "\n")) + 1
	
		local text = ""
		for i = 1, lines do
			text ..= tostring(i) .. "\n"
		end
		lineNumbers.Text = text
	
		local height = lines * lineHeight
	
		-- Resize content ƒë√∫ng s·ªë d√≤ng
		editorBox.Size = UDim2.new(1, 0, 0, height)
		lineNumbers.Size = UDim2.new(1, 0, 0, height)
	
		textScroll.CanvasSize = UDim2.new(0, 0, 0, height)
		lineScroll.CanvasSize = UDim2.new(0, 0, 0, height)
	end
	
	editorBox:GetPropertyChangedSignal("Text"):Connect(updateLines)
	
	textScroll:GetPropertyChangedSignal("CanvasPosition"):Connect(function()
		lineScroll.CanvasPosition = textScroll.CanvasPosition
	end)
	
	updateLines()
	
end
coroutine.wrap(RGVFIY_fake_script)()
local function OLQY_fake_script() -- ExecuteButton.LocalScript 
	local script = Instance.new('LocalScript', ExecuteButton)

	local editorBox = script.Parent.Parent:WaitForChild("TextScroll"):WaitForChild("EditorBox")
	
	script.Parent.MouseButton1Click:Connect(function()
		local code = editorBox.Text
		if code and code ~= "" then
			local func, err = loadstring(code)
			if func then
				local success, result = pcall(func)
				if success then
					print("‚úÖ Executed successfully")
				else
					warn("‚ùå Runtime error:", result)
				end
			else
				warn("‚ùå Compile error:", err)
			end
		else
			warn("‚ö†Ô∏è Nothing to execute.")
		end
	end)
	
end
coroutine.wrap(OLQY_fake_script)()
local function ANXQJQ_fake_script() -- ClearButton.LocalScript 
	local script = Instance.new('LocalScript', ClearButton)

	local editorBox = script.Parent.Parent:WaitForChild("TextScroll"):WaitForChild("EditorBox")
	
	script.Parent.MouseButton1Click:Connect(function()
		editorBox.Text = ""
	end)
	
end
coroutine.wrap(ANXQJQ_fake_script)()
local function ZAUG_fake_script() -- CopyButton.LocalScript 
	local script = Instance.new('LocalScript', CopyButton)

	local editorBox = script.Parent.Parent:WaitForChild("TextScroll"):WaitForChild("EditorBox")
	
	script.Parent.MouseButton1Click:Connect(function()
		if setclipboard then
			setclipboard(editorBox.Text)
			print("üìã Copied to clipboard.")
		else
			warn("‚ùå Clipboard not supported on this executor.")
		end
	end)
	
end
coroutine.wrap(ZAUG_fake_script)()
local function ZZMTIJ_fake_script() -- PasteButton.LocalScript 
	local script = Instance.new('LocalScript', PasteButton)

	local editorBox = script.Parent.Parent:WaitForChild("TextScroll"):WaitForChild("EditorBox")
	
	script.Parent.MouseButton1Click:Connect(function()
		if getclipboard then
			local clip = getclipboard()
			if clip then
				editorBox.Text = clip
				print("üì• Pasted from clipboard.")
			else
				warn("‚ö†Ô∏è Clipboard is empty.")
			end
		else
			warn("‚ùå Clipboard not supported on this executor.")
		end
	end)
	
end
coroutine.wrap(ZZMTIJ_fake_script)()
